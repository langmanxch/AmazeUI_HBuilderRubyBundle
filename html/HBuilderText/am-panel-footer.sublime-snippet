snippet 'am-panel:footer' do |cmd| 
    cmd.trigger = 'am-panel:footer' 
    cmd.needApplyReContentAssist = true 
    cmd.expansion = '
      <section class="am-panel am-panel-default">
        <div class="am-panel-bd">
          ${1:面板内容}
        </div>
        <footer class="am-panel-footer">${2:面板页脚}</footer>
      </section>
      ' 
end 

