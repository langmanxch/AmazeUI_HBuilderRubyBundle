snippet 'am-pagination:right' do |cmd| 
    cmd.trigger = 'am-pagination:right' 
    cmd.needApplyReContentAssist = true 
    cmd.expansion = '
      <ul class="am-pagination am-pagination-right">
        <li class="am-disabled"><a href="#">&laquo;</a></li>
        <li class="am-active"><a href="#">${1:1}</a></li>
        <li><a href="#">${2:2}</a></li>
        <li><a href="#">${3:3}</a></li>
        <li><a href="#">&raquo;</a></li>
      </ul>
      ' 
end 

